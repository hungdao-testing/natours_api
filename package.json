{
  "name": "natours_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "delete-data": "npx cross-env ts-node ./src/main/dev-data/data/import_dev_data.ts --delete",
    "import-data": "npx cross-env ts-node ./src/main/dev-data/data/import_dev_data.ts --import",
    "format": "prettier --write ./src/**/*.ts ./src/public/js/*.js",
    "build": "npx cross-env NODE_ENV=development tsc",
    "start:api:local": "npx cross-env NODE_ENV=local  nodemon ",
    "start:api:dev": "npx cross-env NODE_ENV=development ts-node --files ./src/server.ts",
    "start:api:prod": "npx cross-env NODE_ENV=production nodemon",
    "watch:js:local": "npx cross-env NODE_ENV=local parcel watch --no-hmr ./src/public/js/index.js --out-dir ./src/public/js -o bundle.js --public-url ./",
    "watch:js:dev": "npx cross-env NODE_ENV=development parcel watch --no-hmr ./src/public/js/index.js --out-dir ./src/public/js -o bundle.js --public-url ./",
    "build:js": "npx cross-env NODE_ENV=production parcel build ./src/public/js/index.js --out-dir ./src/public/js --out-file bundle.js",
    "build:app:local": "npx concurrently --kill-others \"npm run watch:js:local\" \"start npm run start:api:local\"",
    "build:app:dev": "npx concurrently --kill-others \"npm run watch:js:dev\" \"start npm run start:api:dev\"",
    "build:app:prod": "npx concurrently --kill-others \"npm run build:js\" \"start npm run start:api:prod\"",
    "test:local:api": "npx cross-env NODE_ENV=local playwright test --project=api --reporter=list,allure-playwright",
    "test:dev:api": "npx cross-env NODE_ENV=development playwright test --project=api --reporter=list,allure-playwright",
    "report:allure:api": "npx cross-env allure open ./reporters/allure/allure-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.20.1",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^8.1.0",
    "@types/lodash": "^4.14.180",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.10",
    "@types/nodemailer": "^6.4.4",
    "@types/rimraf": "^3.0.2",
    "@types/sharp": "^0.30.2",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "allure-commandline": "^2.17.2",
    "allure-playwright": "^2.0.0-beta.15",
    "concurrently": "^7.1.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "parcel-bundler": "^1.3.1",
    "playwright": "^1.20.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
